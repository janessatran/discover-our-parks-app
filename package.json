{
  "name": "discover-our-parks-app",
  "description": "An app that helps you learn more about National Parks in the U.S. that you may not know about!",
  "license": "MIT",
  "author": "Janessa Tran",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "analyze": "cem analyze --litelement",
    "predeploy": "yarn run build",
    "deploy": "gh-pages -d dist",
    "build": "rimraf dist && rollup -c rollup.config.js",
    "start:build": "yarn run build && es-dev-server --root-dir dist --app-index index.html --compatibility none --open"
  },
  "dependencies": {
    "@adobe/lit-mobx": "^2.2.0",
    "@mapbox/mapbox-gl-geocoder": "^2.1.2",
    "@open-wc/building-rollup": "^3.0.2",
    "@vaadin/router": "^1.7.5",
    "deepmerge": "^4.3.1",
    "es-dev-server": "^2.1.0",
    "gh-pages": "^6.0.0",
    "lit": "^2.0.2",
    "mapbox": "^1.0.0-beta9",
    "mapbox-gl": "^0.42.0",
    "mobx": "^6.10.2",
    "rimraf": "^5.0.5",
    "rollup": "^4.5.0"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "^0.4.17",
    "@types/mapbox-gl": "^2.7.18",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@web/dev-server": "^0.1.34",
    "concurrently": "^5.3.0",
    "tslib": "^2.3.1",
    "typescript": "^5.4.0"
  },
  "customElements": "custom-elements.json",
  "main": "index.html",
  "repository": "https://github.com/janessatran/discover-our-parks-app.git"
}
